/*文章页标签*/
.page-tag {
  display: flex; }

.tag, .page-time {
  font-size: 14px;
  margin: 0 10px 0 0;
  border-radius: 4px;
  background: #FFECE6;
  color: #444; }

.tag {
  display: flex;
  padding: 10px;
  font-weight: bold; }

.tag a {
  color: #444; }

.page-time {
  padding: 12px;
  float: left; }

@media (prefers-color-scheme: dark) {
  .tag, .page-time {
    background: #332E2F !important;
    color: #BBBBBB !important; }
